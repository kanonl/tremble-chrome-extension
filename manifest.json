{
    "manifest_version": 2,
    "name": "tremble",
    "version": "0.0.109",
    "description": "tremble lets you track the Twitch channels that you follow and notifies you when they go live.",
    "icons": {
        "16": "assets/images/twitch-16.png",
        "32": "assets/images/twitch-32.png",
        "48": "assets/images/twitch-48.png",
        "96": "assets/images/twitch-96.png",
        "128": "assets/images/twitch-128.png"
    },
    "author": "Kanon Lo",
    "browser_action": {
        "default_icon": {
            "16": "assets/images/twitch-16.png",
            "24": "assets/images/twitch-24.png",
            "32": "assets/images/twitch-32.png"
        },
        "default_title": "tremble",
        "default_popup": "assets/views/popup.html"
    },
    "options_page": "assets/views/options.html",
    "background": {
        "scripts": [
            "assets/js/background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "alarms",
        "notifications",
        "storage",
        "https://api.twitch.tv/",
        "https://static-cdn.jtvnw.net/"
    ]
}